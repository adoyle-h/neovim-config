{
  "diagnostic.displayByAle": true,
  "yank.highlight.enable": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "list.source.files.excludePatterns": [
    "**/vendor/**",
    "**/node_modules/**",
    "**/bower_components/**",
    "**/.git/**",
  ],
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-x>": "do:stop",
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit",
  },
  "list.normalMappings": {
  },
  "coc.preferences.extensionUpdateCheck": "weekly",
  "tabnine.priority": 999,
}
